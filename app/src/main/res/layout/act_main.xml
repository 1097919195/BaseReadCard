    </FrameLayout>

    <LinearLayout
        android:id="@+id/tab_navigation"
        android:layout_alignParentRight="true"
        android:layout_width="50dp"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <Button
            android:id="@+id/bindingWithSample"
            android:background="@drawable/btn_tab_navigation"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:text="样衣绑定"
            android:textSize="@dimen/item_text_size_25"
            android:textColor="@color/white"
            android:ems="1"/>
        <Button
            android:id="@+id/bindingWithStaff"
            android:background="@drawable/btn_tab_navigation"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:text="员工绑定"
            android:textSize="@dimen/item_text_size_25"
            android:textColor="@color/white"
            android:ems="1"/>
        <Button
            android:id="@+id/bindingWithGoods"
            android:background="@drawable/btn_tab_navigation"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:text="物品绑定"
            android:textSize="@dimen/item_text_size_25"
            android:textColor="@color/white"
            android:ems="1"/>
        <Button
            android:id="@+id/assignStore"
            android:background="@drawable/btn_tab_navigation"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:text="门店分配"
            android:textSize="@dimen/item_text_size_25"
            android:textColor="@color/white"
            android:ems="1"/>
    </LinearLayout>

